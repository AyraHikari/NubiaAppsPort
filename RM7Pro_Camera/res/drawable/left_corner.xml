<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#18000000" />
            <corners android:topLeftRadius="@dimen/dimens_7" android:bottomLeftRadius="@dimen/dimens_7" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#00000000" />
            <corners android:topLeftRadius="@dimen/dimens_7" android:topRightRadius="@dimen/dimens_7" android:bottomLeftRadius="@dimen/dimens_7" android:bottomRightRadius="@dimen/dimens_7" />
        </shape>
    </item>
</selector>