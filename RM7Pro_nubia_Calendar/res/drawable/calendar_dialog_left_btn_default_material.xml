<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?android:colorControlHighlight"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_enabled="false">
                <nine-patch android:src="@drawable/calendar_dialog_left_btn_default_normal_mtrl" android:tint="@color/calendar_disabled_light" />
            </item>
            <item android:state_pressed="true" android:drawable="@drawable/calendar_dialog_left_btn_default_pressed_mtr" />
            <item>
                <nine-patch android:src="@drawable/calendar_dialog_left_btn_default_normal_mtrl" android:tint="@color/calendar_normal_secondary_light" />
            </item>
        </selector>
    </item>
</ripple>