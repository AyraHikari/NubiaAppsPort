<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="@dimen/dimen_6" android:color="@color/color_black_transparent_20" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="@dimen/dimen_6" android:color="@color/cf25252" />
            </shape>
        </clip>
    </item>
</layer-list>