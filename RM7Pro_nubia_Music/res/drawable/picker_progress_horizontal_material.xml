<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <nine-patch android:src="@drawable/picker_progress_mtrl_alpha" android:tint="@color/picker_color_black_alpha_ad" android:alpha="@dimen/picker_disabled_text_alpha_26" />
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/picker_progress_mtrl_alpha" android:tint="@color/picker_color_activated_red" android:alpha="@dimen/picker_disabled_text_alpha_26" />
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/picker_progress_mtrl_alpha" android:tint="@color/picker_color_activated_red" />
        </scale>
    </item>
</layer-list>