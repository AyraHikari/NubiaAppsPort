<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.Light.ActionBar</item>
        <item name="android:displayOptions">none</item>
    </style>
    <style name="CursorSeekBar" parent="@android:style/Widget.Holo.Light.SeekBar">
        <item name="android:maxHeight">100.0dip</item>
        <item name="android:progressDrawable">@drawable/seek_bar_progress</item>
        <item name="android:thumb">@drawable/seek_bar_thumb</item>
        <item name="android:thumbOffset">0.0dip</item>
        <item name="android:paddingStart">0.0dip</item>
        <item name="android:paddingEnd">0.0dip</item>
    </style>
    <style name="DialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:backgroundDimAmount">0.65</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="FilterIconButton">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textSize">@dimen/small_text_size</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">@dimen/thumbnail_width</item>
        <item name="android:layout_height">@dimen/thumbnail_height</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">3.0dip</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="FilterShowBottomButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:paddingBottom">16.0dip</item>
        <item name="android:layout_width">96.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="FilterShowHistoryButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:padding">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FilterShowSlider">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">13.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/filtershow_slider</item>
        <item name="android:progressDrawable">@drawable/filtershow_slider</item>
        <item name="android:minHeight">13.0dip</item>
        <item name="android:thumb">@drawable/filtershow_scrubber</item>
        <item name="android:thumbOffset">16.0dip</item>
        <item name="android:paddingStart">16.0dip</item>
        <item name="android:paddingEnd">16.0dip</item>
    </style>
    <style name="FilterShowSliderEx">
        <item name="android:focusable">true</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:paddingStart">16.0dip</item>
        <item name="android:paddingEnd">16.0dip</item>
    </style>
    <style name="FilterShowTopButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:padding">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="Holo.Light.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/actionbar_translucent</item>
        <item name="android:displayOptions">showHome|useLogo</item>
        <item name="android:backgroundStacked">@null</item>
    </style>
    <style name="LoadingProgressBarLarge">
        <item name="android:maxWidth">24.0dip</item>
        <item name="android:maxHeight">24.0dip</item>
        <item name="android:minWidth">24.0dip</item>
        <item name="android:minHeight">24.0dip</item>
    </style>
    <style name="MainSetOptionText">
        <item name="android:textSize">@dimen/main_set_text_size</item>
        <item name="android:textColor">@color/ninety_white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/collage_view_left_padding</item>
        <item name="android:layout_marginBottom">@dimen/main_set_text_padding_bottom</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="MainSetRelativeLayout">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/main_set_item_height</item>
    </style>
    <style name="MainSetRightArrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/collage_activity_actionbar_right_padding</item>
        <item name="android:layout_marginBottom">@dimen/main_set_arrow_padding_bottom</item>
        <item name="android:src">@drawable/set_right_arrow</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="NubiaFontMedium">
        <item name="android:fontFamily">nubiafont-medium</item>
        <item name="android:elegantTextHeight">true</item>
    </style>
    <style name="PhotoEditorFullTheme" parent="@style/PhotoEditorTheme">
        <item name="android:windowBackground">@color/default_background</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="PhotoEditorTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:seekBarStyle">@style/Widget.Nubia.SeekBar</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="PhotoGifTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:buttonStyle">@style/Widget.Nubia.Button</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:seekBarStyle">@style/Widget.Nubia.SeekBar</item>
        <item name="android:radioButtonStyle">@style/Widget.Nubia.CompoundButton.RadioButton</item>
        <item name="android:listChoiceIndicatorSingle">@drawable/nubia_btn_radio_material_anim</item>
    </style>
    <style name="SegmentLine">
        <item name="android:background">@color/ten_white</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_marginLeft">@dimen/collage_view_left_padding</item>
    </style>
    <style name="Switch">
        <item name="android:background">@null</item>
        <item name="showTextnew">false</item>
        <item name="switchMinHightnew">48.0dip</item>
        <item name="switchMinWidthnew">46.0dip</item>
        <item name="switchTextAppearancenew">?android:textAppearance</item>
        <item name="textOffnew">@string/cancel</item>
        <item name="textOnnew">@string/cancel</item>
        <item name="thumbnew">@drawable/nubia_switch_thumb_material</item>
        <item name="tracknew">@drawable/nubia_switch_track_material</item>
    </style>
    <style name="TextAppearance.Compat.Notification" parent="@android:style/TextAppearance.Material.Notification" />
    <style name="TextAppearance.Compat.Notification.Info" parent="@android:style/TextAppearance.Material.Notification.Info" />
    <style name="TextAppearance.Compat.Notification.Info.Media" parent="@style/TextAppearance.Compat.Notification.Info">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Line2" parent="@style/TextAppearance.Compat.Notification.Info" />
    <style name="TextAppearance.Compat.Notification.Line2.Media" parent="@style/TextAppearance.Compat.Notification.Info.Media" />
    <style name="TextAppearance.Compat.Notification.Media" parent="@style/TextAppearance.Compat.Notification">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Time" parent="@android:style/TextAppearance.Material.Notification.Time" />
    <style name="TextAppearance.Compat.Notification.Time.Media" parent="@style/TextAppearance.Compat.Notification.Time">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Title" parent="@android:style/TextAppearance.Material.Notification.Title" />
    <style name="TextAppearance.Compat.Notification.Title.Media" parent="@style/TextAppearance.Compat.Notification.Title">
        <item name="android:textColor">@color/primary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Editor.ActionBarTittle" parent="@style/TextAppearance.Nubia.Large">
        <item name="android:textSize">@dimen/nubia_text_size_medium_no_change</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearance.Nubia.AlertDialog.CheckedTextView" parent="@style/TextAppearance.Nubia.Medium">
        <item name="android:textSize">@dimen/nubia_text_size_medium</item>
        <item name="android:textColor">@color/nubia_alert_dialog_list_item_color</item>
    </style>
    <style name="TextAppearance.Nubia.AlertDialogTitle" parent="@style/TextAppearance.Nubia.Small">
        <item name="android:textSize">@dimen/nubia_dialog_title_text_size</item>
    </style>
    <style name="TextAppearance.Nubia.Large" parent="@android:style/TextAppearance.Material.Large">
        <item name="android:textSize">@dimen/nubia_text_size_large</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="TextAppearance.Nubia.Medium" parent="@android:style/TextAppearance.Material.Medium">
        <item name="android:textSize">@dimen/nubia_text_size_medium</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="TextAppearance.Nubia.Small" parent="@android:style/TextAppearance.Material.Small">
        <item name="android:textSize">@dimen/nubia_text_size_small</item>
        <item name="android:textColor">?android:textColorSecondary</item>
    </style>
    <style name="Theme.FullScreen" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="ThemeDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:statusBarColor">@color/white</item>
    </style>
    <style name="Widget.Compat.NotificationActionContainer" parent="">
        <item name="android:background">@drawable/notification_action_background</item>
    </style>
    <style name="Widget.Compat.NotificationActionText" parent="">
        <item name="android:textAppearance">?android:textAppearanceButton</item>
        <item name="android:textSize">@dimen/notification_action_text_size</item>
        <item name="android:textColor">@color/secondary_text_default_material_light</item>
    </style>
    <style name="Widget.Nubia.Button" parent="@android:style/Widget.Material.Light.Button">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:background">@drawable/nubia_btn_default_material</item>
        <item name="android:stateListAnimator">@null</item>
    </style>
    <style name="Widget.Nubia.CompoundButton.RadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
        <item name="android:button">?android:listChoiceIndicatorSingle</item>
    </style>
    <style name="Widget.Nubia.SeekBar" parent="@android:style/Widget.Material.Light.SeekBar">
        <item name="android:background">@drawable/nubia_seekbar_background</item>
        <item name="android:indeterminateDrawable">@drawable/nubia_scrubber_progress_horizontal_material</item>
        <item name="android:progressDrawable">@drawable/nubia_scrubber_progress_horizontal_material</item>
        <item name="android:thumb">@drawable/nubia_scrubber_control_material_anim</item>
    </style>
    <style name="anim_popup_dir">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>
    <style name="bottom_btn_item">
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="common_ui_divider">
        <item name="android:background">@color/separate_gray</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0px</item>
    </style>
    <style name="common_ui_linelayout">
        <item name="android:orientation">vertical</item>
        <item name="android:background">#ffffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/common_dialog_height</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="common_ui_linelayout_delete">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="common_ui_linelayout_rename">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/common_dialog_height_rename</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="common_ui_title">
        <item name="android:textSize">@dimen/common_dialog_title_tvsize_delete</item>
        <item name="android:textColor">#91000000</item>
        <item name="android:background">#ffffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:editable">false</item>
    </style>
    <style name="dialog_anim">
        <item name="android:windowEnterAnimation">@anim/dialog_in</item>
        <item name="android:windowExitAnimation">@anim/dialog_out</item>
    </style>
</resources>
