<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?android:colorControlHighlight"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask" android:drawable="@drawable/nubia_dialog_left_btn_mask" />
    <item>
        <selector>
            <item android:state_pressed="true">
                <nine-patch android:src="@drawable/nubia_dialog_left_btn_default_pressed" />
            </item>
            <item android:state_enabled="false">
                <nine-patch android:src="@drawable/nubia_dialog_left_btn_default_disabled" />
            </item>
            <item>
                <nine-patch android:src="@drawable/nubia_dialog_left_btn_default_normal" />
            </item>
        </selector>
    </item>
</ripple>