<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/nubia_btn_radio_mtrl_checked_disabled" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/nubia_btn_radio_mtrl_disabled" />
    </item>
    <item android:state_checked="true" android:id="@id/nubia_on">
        <bitmap android:src="@drawable/nubia_btn_radio_mtrl_017" />
    </item>
    <item android:id="@id/nubia_off">
        <bitmap android:src="@drawable/nubia_btn_radio_mtrl_000" />
    </item>
    <transition android:toId="@id/nubia_on" android:fromId="@id/nubia_off">
        <animation-list>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_000" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_001" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_002" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_003" />
            </item>
            <item android:duration="15">
