            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_005" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_006" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_007" />
            </item>
            <item android:duration="15">
