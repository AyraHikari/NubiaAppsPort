            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_011" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_012" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_013" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_014" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_015" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_016" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_017" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@id/nubia_off" android:fromId="@id/nubia_on">
        <animation-list>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_017" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_016" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/nubia_btn_radio_mtrl_015" />
